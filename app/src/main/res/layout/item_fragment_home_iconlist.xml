<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_marginTop="@dimen/x6"
    android:layout_width="match_parent"
    android:layout_height="@dimen/x60">
    <ImageView
        android:id="@+id/item_fragment_home_iconlist_icon"
        android:layout_width="@dimen/x40"
        android:layout_height="@dimen/x40"
        android:layout_centerHorizontal="true"
        android:scaleType="fitCenter"
        android:src="@mipmap/icon_find_zhaoyao" />

    <TextView
        android:id="@+id/item_fragment_home_iconlist_name"
        style="@style/font_ww_14"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:text="@string/holder"
        android:textColor="@color/font_black2" />

</RelativeLayout>
