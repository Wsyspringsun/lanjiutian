<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center"
    android:layout_marginTop="@dimen/vertical_margin"
    android:layout_marginBottom="@dimen/vertical_margin"
    android:orientation="vertical">


    <ImageView
        android:id="@+id/item_fragment_home_iconlist_icon"
        android:layout_width="@dimen/x38"
        android:layout_height="@dimen/x38"
        android:layout_marginBottom="@dimen/vertical_margin"
        android:scaleType="fitXY"
        android:src="@drawable/ic_consult" />

    <TextView
        android:id="@+id/item_fragment_home_iconlist_name"
        style="@style/font_ww_14"
        android:text="@string/holder"
        android:textColor="@color/font_1" />


</LinearLayout>
